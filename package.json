{
  "name": "real-time-eval",
  "displayName": "real-time eval",
  "description": "evalutes code in real-time",
  "version": "0.0.1",
  "publisher": "Almenon",
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.evalPythonInRealTime"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.evalPythonInRealTime",
        "title": "eval python in real time"
      }
    ]
  },
  "keybindings": [
    {
      "command": "extension.evalPythonInRealTime",
      "key": "ctrl+q s",
      "mac": "ctrl+q s",
      "when": "editorTextFocus"
    }
  ],
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "arepl-backend": "^1.0.0-alpha.4",
    "opn": "^4.0.2",
    "path": "^0.12.7",
    "python-shell": "^0.4.0"
  }
}
